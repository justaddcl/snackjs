type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  admin: Boolean!
  dietaryRestrictions: String
  favoriteSnack: String
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type UserRole {
  id: ID! @id
  type: String!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}